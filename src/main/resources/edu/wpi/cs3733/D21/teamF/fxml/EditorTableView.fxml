<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Tab?>

<JFXTabPane fx:id="tabPane" prefHeight="368.0" prefWidth="300.0" stylesheets="/stylesheets/MapEditorTabViewStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D21.teamF.controllers.EditorTableController">
   <tabs>
      <Tab fx:id="nodesTab" text="Nodes">
         <content>
            <JFXTreeTableView fx:id="nodeTreeTable" onMouseClicked="#handleSelectNode" stylesheets="/stylesheets/NodeEditorTreeTableStyle.css" />
         </content>
      </Tab>
      <Tab fx:id="edgesTab" text="Edges">
         <content>
            <JFXTreeTableView fx:id="edgeTreeTable" onMouseClicked="#handleSelectEdge" stylesheets="/stylesheets/NodeEditorTreeTableStyle.css" />
         </content>
      </Tab>
   </tabs>
</JFXTabPane>
