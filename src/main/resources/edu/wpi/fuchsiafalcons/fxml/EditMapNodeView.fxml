<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: cde7ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.fuchsiafalcons.controllers.EditMapNodeController">
   <children>
      <VBox alignment="CENTER" prefHeight="540.0" prefWidth="960.0" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
               <children>
                  <Label text="Map Vertex Editor">
                     <font>
                        <Font name="Tahoma" size="24.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="694.0" prefWidth="1900.0">
               <children>
                  <JFXTreeTableView fx:id="nodeTreeTable" onMouseClicked="#handleSelectNode" prefHeight="694.0" prefWidth="216.0" stylesheets="/stylesheets/NodeEditorTreeTableStyle.css" />
                  <ScrollPane fx:id="scroll" prefHeight="694.0" prefWidth="1267.0" stylesheets="/stylesheets/NodeEditorScrollPaneStyle.css">
                     <content>
                        <Pane fx:id="canvas" prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <ImageView fx:id="map" fitHeight="694.0" fitWidth="1267.0" onContextMenuRequested="#handleMapContextMenu" pickOnBounds="true" preserveRatio="true" />
                           </children>
                        </Pane>
                     </content>
                  </ScrollPane>
                  <VBox alignment="CENTER" prefHeight="427.0" prefWidth="207.0" spacing="20.0">
                     <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                           <children>
                              <Label text="Floor:" textAlignment="CENTER">
                                 <font>
                                    <Font name="Tahoma" size="13.0" />
                                 </font>
                              </Label>
                              <JFXComboBox fx:id="floorComboBox" onAction="#handleFloorBoxAction" />
                           </children>
                        </HBox>
                        <ImageView fx:id="logoView" fitHeight="168.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <JFXTextField fx:id="filenameField" alignment="CENTER" focusColor="#1d26c2" onKeyReleased="#handleFileNameType" promptText="Filename Here" style="-fx-background-color: #ced7dd;">
                           <VBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </VBox.margin>
                           <font>
                              <Font name="Tahoma" size="13.0" />
                           </font>
                        </JFXTextField>
                        <JFXButton fx:id="saveToFileButton" buttonType="RAISED" disable="true" onAction="#handleSaveButtonClicked" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="Save to File" textFill="WHITE">
                           <font>
                              <Font name="Tahoma" size="13.0" />
                           </font></JFXButton>
                        <JFXButton fx:id="loadFromFileButton" buttonType="RAISED" onAction="#handleLoadButtonClicked" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="Load From File" textFill="WHITE">
                           <font>
                              <Font name="Tahoma" size="13.0" />
                           </font></JFXButton>
                        <JFXButton fx:id="resetDatabaseButton" buttonType="RAISED" onAction="#handleResetDatabase" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="Reset Database" textFill="WHITE">
                           <font>
                              <Font name="Tahoma" size="13.0" />
                           </font>
                        </JFXButton>
                        <Label fx:id="errorMessageLabel" />
                     </children>
                     <padding>
                        <Insets left="100.0" />
                     </padding>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="58.0" prefWidth="940.0" spacing="50.0">
               <children>
                  <JFXButton fx:id="newButton" buttonType="RAISED" onAction="#handleNewNode" prefHeight="40.0" prefWidth="75.0" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="New" textFill="WHITE">
                     <font>
                        <Font name="Tahoma" size="18.0" />
                     </font></JFXButton>
                  <JFXButton fx:id="editButton" buttonType="RAISED" onAction="#handleEditNode" prefHeight="40.0" prefWidth="75.0" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="Edit" textFill="WHITE">
                     <font>
                        <Font name="Tahoma" size="18.0" />
                     </font></JFXButton>
                  <JFXButton fx:id="deleteButton" buttonType="RAISED" onAction="#handleDeletePushed" prefHeight="40.0" prefWidth="86.0" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="Delete" textFill="WHITE">
                     <font>
                        <Font name="Tahoma" size="18.0" />
                     </font></JFXButton>
                  <HBox alignment="CENTER" prefHeight="38.0" prefWidth="186.0" spacing="5.0">
                     <children>
                        <Label text="Zoom Level:">
                           <font>
                              <Font name="Tahoma" size="18.0" />
                           </font>
                        </Label>
                        <JFXButton fx:id="zoomInButton" buttonType="RAISED" onAction="#handleZoom" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="+" textFill="WHITE">
                           <font>
                              <Font size="19.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="zoomOutButton" buttonType="RAISED" disable="true" onAction="#handleZoom" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="-" textFill="WHITE">
                           <font>
                              <Font size="19.0" />
                           </font>
                        </JFXButton>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <JFXButton fx:id="goBack" buttonType="RAISED" layoutX="10.0" layoutY="14.0" onAction="#handleBackButtonPushed" prefHeight="23.0" prefWidth="35.0" ripplerFill="WHITE" style="-fx-background-color: #1C77C3;" text="X" textFill="WHITE">
         <font>
            <Font name="Tahoma" size="13.0" />
         </font></JFXButton>
      <JFXTextField fx:id="searchField" alignment="CENTER" onKeyReleased="#handleSearchNode" prefHeight="27.0" prefWidth="150.0" promptText="Search Node By:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="150.0" />
      <JFXComboBox fx:id="searchComboBox" AnchorPane.leftAnchor="170.0" AnchorPane.topAnchor="150.0" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
