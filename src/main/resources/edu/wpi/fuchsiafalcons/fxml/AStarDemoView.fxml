<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="540.0" prefWidth="960.0" style="-fx-background-color: cde7ff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.fuchsiafalcons.controllers.AStarDemoController">
   <children>
      <VBox alignment="CENTER" layoutX="272.0" layoutY="35.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label text="AStar PathFinding Demo">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <ScrollPane fx:id="scroll" prefHeight="450.0" prefWidth="700.0">
               <content>
                  <Pane fx:id="canvas" maxHeight="-Infinity" maxWidth="-Infinity" onMousePressed="#drawLine" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="map" fitHeight="170.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </Pane>
               </content>
            </ScrollPane>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ComboBox fx:id="startComboBox" onAction="#handleStartBoxAction" prefWidth="150.0" promptText="Select Start Location" />
                  <ComboBox fx:id="endComboBox" onAction="#handleEndBoxAction" prefWidth="150.0" promptText="Select End Location">
                     <padding>
                        <Insets left="16.0" />
                     </padding>
                  </ComboBox>
                  <Button fx:id="pathfindButton" mnemonicParsing="false" onAction="#handlePathfindButtonClicked" text="Go" />
               </children>
            </HBox>
         </children></VBox>
      <Button fx:id="goBack" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleButtonPushed" text="X">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
